{
    "compilerOptions": {
        "noImplicitAny": true,
        "strict": true,
        "module": "CommonJS",
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "target": "ES2020",
        "strictNullChecks": true,
        "sourceMap": true,
        "noImplicitReturns": true,
        "strictFunctionTypes": true,
        "strictBindCallApply": true,
        "strictPropertyInitialization": true,
        "forceConsistentCasingInFileNames": true,
        "alwaysStrict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "allowUnreachableCode": false,
        "allowUnusedLabels": false,
        "moduleResolution": "node",
        "baseUrl": "./",
        "paths": {
            "~/*": [
                "./*"
            ],
            "@core": [
                "./src/core"
            ],
            "@core/*": [
                "./src/core/*"
            ],
            "@cluster": [
                "./src/workers/cluster",
            ],
            "@cluster/*": [
                "./src/workers/cluster/*"
            ],
            "@master": [
                "./src/workers/master"
            ],
            "@master/*": [
                "./src/workers/master/*"
            ],
            "@image": [
                "./src/workers/image"
            ],
            "@image/*": [
                "./src/workers/image/*"
            ],
            "@website": [
                "./src/workers/website"
            ],
            "@website/*": [
                "./src/workers/website/*"
            ],
            "@website-old": [
                "./src/workers/website_old"
            ],
            "@website-old/*": [
                "./src/workers/website_old/*"
            ]
        }
        // "noUncheckedIndexedAccess": true
    }
}