{
    "compilerOptions": {
        "noImplicitAny": true,
        "strict": true,
        "module": "CommonJS",
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "target": "ESNext",
        "strictNullChecks": true,
        "sourceMap": true,
        "noImplicitReturns": true,
        "strictFunctionTypes": true,
        "strictBindCallApply": true,
        "strictPropertyInitialization": true,
        "forceConsistentCasingInFileNames": true,
        "experimentalDecorators": true,
        "alwaysStrict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "allowUnreachableCode": false,
        "allowUnusedLabels": false,
        "moduleResolution": "node",
        "types": [],
        "lib": [
            "ES2021"
        ],
        "paths": {
            "@blargbot/core": [
                "./src/core"
            ],
            "@blargbot/core/*": [
                "./src/core/*"
            ],
            "@blargbot/cluster": [
                "./src/cluster",
            ],
            "@blargbot/cluster/*": [
                "./src/cluster/*"
            ],
            "@blargbot/master": [
                "./src/master"
            ],
            "@blargbot/master/*": [
                "./src/master/*"
            ],
            "@blargbot/image": [
                "./src/image"
            ],
            "@blargbot/image/*": [
                "./src/image/*"
            ],
            "@blargbot/api": [
                "./src/api"
            ],
            "@blargbot/api/*": [
                "./src/api/*"
            ],
            "@blargbot/config": [
                "./config.json"
            ],
            "@blargbot/package": [
                "./package.json"
            ],
            "@blargbot/res/*": [
                "./res/*"
            ]
        }
    }
}