{
    "compilerOptions": {
        "noImplicitAny": true,
        "strict": true,
        "module": "CommonJS",
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "target": "ES2021",
        "strictNullChecks": true,
        "sourceMap": true,
        "noImplicitReturns": true,
        "strictFunctionTypes": true,
        "strictBindCallApply": true,
        "strictPropertyInitialization": true,
        "forceConsistentCasingInFileNames": true,
        "alwaysStrict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "allowUnreachableCode": false,
        "allowUnusedLabels": false,
        "moduleResolution": "node",
        "lib": [
            "ES2021"
        ],
        // "noUncheckedIndexedAccess": true
        "paths": {
            "@core": [
                "./src/core"
            ],
            "@core/*": [
                "./src/core/*"
            ],
            "@cluster": [
                "./src/workers/cluster",
            ],
            "@cluster/*": [
                "./src/workers/cluster/*"
            ],
            "@master": [
                "./src/workers/master"
            ],
            "@master/*": [
                "./src/workers/master/*"
            ],
            "@image": [
                "./src/workers/image"
            ],
            "@image/*": [
                "./src/workers/image/*"
            ],
            "@website": [
                "./src/workers/website"
            ],
            "@website/*": [
                "./src/workers/website/*"
            ],
            "@workers/*": [
                "./src/workers/*"
            ],
            "@config": [
                "./config.json"
            ],
            "@package": [
                "./package.json"
            ],
            "@res/*": [
                "./res/*"
            ]
        }
    }
}