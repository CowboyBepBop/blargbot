        "@types/mocha": "^9.0.0",
        "@types/module-alias": "^2.0.1",
        "@types/node": "^16.11.11",
        "@types/node-fetch": "^2.5.12",
        "@types/pg": "^8.6.1",
        "@types/phantom": "^3.2.6",
        "@types/rwlock": "^5.0.3",
        "@types/safe-regex": "^1.1.3",
        "@types/sequelize": "^4.28.10",
        "@types/svg2png": "^4.1.1",
        "@types/tinycolor2": "^1.4.3",
        "@types/twemoji": "^12.1.2",
        "@types/unorm": "^1.3.28",
        "@types/ws": "^8.2.2",
        "@types/xml2js": "^0.4.9",
        "@typescript-eslint/eslint-plugin": "^5.5.0",
