        "discord-api-types": "^0.25.2",
        "eslint": "^8.3.0",
        "eslint-plugin-import": "^2.25.3",
        "eslint-plugin-simple-import-sort": "^7.0.0",
        "husky": "^7.0.4",
        "mocha": "^9.1.3",
        "nyc": "^15.1.0",
        "sass-lint": "^1.12.1",
        "ts-mockito": "^2.6.1",
        "ts-node": "^10.4.0",
        "typescript": "^4.5.2",
        "yarn": "^1.22.17"
    },
    "scripts": {
        "filecount": "cloc ./ --exclude-dir=node_modules --exclude-ext=js,msj,cjs",
        "postinstall": "sed -i \"/^declare \\(var\\|function\\)/d\" ./node_modules/@types/mocha/index.d.ts && echo dirty dirty mocha",
        "start": "node index.js",
        "test": "mocha --config mocharc.json --recursive --parallel --reporter dot",
        "check": "yarn rebuild && yarn lint && yarn test",
        "ci": "yarn rebuild && yarn lint && yarn coverage",
        "coverage": "nyc yarn test",
        "build": "tsc",
        "build:watch": "tsc -w",
        "rebuild": "yarn clean && yarn build",
        "lint": "eslint --ext .ts,.mts,.cts ./",
        "prepare": "husky install",
        "clean": "del-cli \"**/*.js\" \"**/*.js.map\" \"**/*.mjs\" \"**/*.mjs.map\" \"**/*.cjs\" \"**/*.cjs.map\" \"!node_modules\" \"coverage/\" \".nyc_output/\""
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/blargbot/blargbot"
    },
    "author": "stupid cat (ratismal)",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/blargbot/blargbot/issues"
    },
    "homepage": "https://github.com/blargbot/blargbot#readme",
    "_moduleAliases": {
        "@config": "config.json",
        "@package": "package.json",
        "@res": "res",
        "@core": "src/core",
        "@cluster": "src/workers/cluster",
        "@master": "src/workers/master",
        "@image": "src/workers/image",
        "@api": "src/workers/api",
        "@workers": "src/workers"
    }
}